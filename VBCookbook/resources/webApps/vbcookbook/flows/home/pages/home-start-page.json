{

  "title": "",
  "description": "",
  "variables": {
    "recipeListArray": {
      "type": "any",
      "defaultValue": "{{ $page.functions.getRecipeListData( $application.functions.getMetadata() ) }}"
    },
    "recipeListADP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "data": "{{ $page.variables.recipeListArray }}",
        "itemType": "any"
      }
    }
  },
  "metadata": {},
  "types": {},
  "chains": {
  },
  "eventListeners": {
    "aClick": {
      "chains": [{
        "chainId": "RecipeTileClicked",
        "parameters": {
          "detail": "{{ $event.detail }}",
          "key": "{{ $current.key }}",
          "index": "{{ $current.index }}",
          "current": "{{ $current }}"
        }
      }]
    },
    "openInVBActionClicked": {
      "preventDefault": "true",
      "chains": [{
        "chainId": "OpenHowToOpenVBDialog"
      }]
    }
  },
  "imports": {
    "components": {
      "oj-action-card": {
        "path": "ojs/ojactioncard"
      },
      "oj-list-view": {
        "path": "ojs/ojlistview"
      }
    }
  }
}