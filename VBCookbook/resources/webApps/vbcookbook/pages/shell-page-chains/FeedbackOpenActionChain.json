{
  "description": "",
  "root": "resetFeedback",
  "actions": {
    "callComponentFeedbackDialogOpen": {
      "module": "vb/action/builtin/callComponentMethodAction",
      "parameters": {
        "component": "{{ document.getElementById('feedback-dialog') }}",
        "method": "open"
      }
    },
    "resetFeedback": {
      "module": "vb/action/builtin/resetVariablesAction",
      "parameters": {
        "variables": [
          "$page.variables.feedback"
        ]
      },
      "outcomes": {
        "success": "callComponentFeedbackDialogOpen"
      }
    }
  },
  "variables": {
    "detail": {
      "required": true,
      "type": "any",
      "input": "fromCaller"
    }
  }
}